        <add name="vui3-service"
            virtualUrl="^~/vui(\d?)/services/([A-z0-9\-!\?_]*)(\/?)$"
            rewriteUrlParameter="ExcludeFromClientQueryString"
            destinationUrl="~/vui/services/$2"
            ignoreCase="true"
    />

