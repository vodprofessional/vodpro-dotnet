        ignoreCase="true"
        />-->



      <!-- VUI3 URLs -->
      <!-- Services Page does not land anywhere - go to screenshots -->
        <add name="vui3-services"
            virtualUrl="^~/vui(\d?)/services(\/?)$"
            rewriteUrlParameter="ExcludeFromClientQueryString"
            destinationUrl="~/vui/screenshots/"
            ignoreCase="true"
    />
